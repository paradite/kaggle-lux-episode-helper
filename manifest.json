{
  "name": "Lux AI Episode Helper",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Lux AI Episode Helper"
  },
  "content_scripts": [
    {
      "matches": ["*://www.kaggle.com/*"],
      "css": ["myStyles.css"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["https://www.kaggle.com/*"]
}
